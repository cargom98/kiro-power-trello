{
  "mcpServers": {
     "trello": {
      "command": "uvx",
      "args": [
        "--from",
        "trello-mcp-server",
        "trello_mcp_server"
      ],
      "env": {
        "TRELLO_API_KEY": ""
      },
      "autoApprove": [
        "list_boards",
        "get_board",
        "list_board_lists",
        "list_board_cards",
        "get_card",
        "list_organizations",
        "list_organization_boards",
        "list_organization_members"
      ]
    }
  }
}